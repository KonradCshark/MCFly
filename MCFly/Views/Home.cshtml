@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@using System.Reflection;
@{
    Layout = "Master.cshtml";
}



@if (Model.Content.HasValue("mCFly"))
{
    @Html.Action("RenderForm", "MCFlyEntries", new { alias = Model.Content.GetPropertyValue<string>("mCFly") })	@*, view= "MCFly/MCFlyAjaxForm" *@
}

